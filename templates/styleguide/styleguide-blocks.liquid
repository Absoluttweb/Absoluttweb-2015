<!-- Block-11 -->
<script>
	var container11 = $('#block-11').imagesLoaded( function() {;
		container11.isotope({
			itemSelector: '.item',
			layoutMode: 'masonry',
			transitionDuration: '300ms',
			masonry: {
				columnWidth: '.item',
				gutter: 0
			}
		})
	});
</script>
<!-- END Block-11 -->

